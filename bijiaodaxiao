#include "stdio.h" 
void main() 
{ 
int paixu(int a,int b,int c); 
int a1,a2,a3,m; 
scanf("%d %d %d",&a1,&a2,&a3); 
m=paixu(a1,a2,a3); 
printf("%d",m); 
} 
int paixu(int a,int b,int c) 
{ 
int max(int x,int y); 
int m; 
m=max(a,b);  
m=max(m,c); 
} 
int max(int x,int y) 
{ 
if(x>y) return x; 
else 
return y; 
}


